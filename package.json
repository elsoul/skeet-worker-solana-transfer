{
  "name": "skeet-worker-solana-transfer",
  "version": "0.2.1",
  "description": "Skeet Worker Plugin Solana Token Transfer",
  "main": "dist/index.js",
  "repository": "https://github.com/elsoul/skeet-worker-solana-transfer.git",
  "author": "POPPIN-FUMI",
  "private": true,
  "scripts": {
    "test": "jest --coverage=false --detectOpenHandles --maxWorkers=1",
    "update:packages": "ncu -u && yarn",
    "build": "npx ts-node build.ts",
    "publish": "npm publish",
    "dev": "nodemon",
    "start": "node dist/index.js",
    "d:login": "cat ./keyfile.json | docker login -u _json_key --password-stdin https://eu.gcr.io",
    "d:build": "docker build . --platform linux/amd64 -t skeet-worker-solana-transfer",
    "d:tag": "docker tag skeet-worker-solana-transfer eu.gcr.io/skeet-framework/skeet-worker-solana-transfer",
    "d:push": "docker push eu.gcr.io/skeet-framework/skeet-worker-solana-transfer",
    "gcp:project": "gcloud config set project skeet-framework",
    "deploy": "yarn gcp:project && yarn d:login && yarn d:build && yarn d:tag && yarn d:push",
    "crypto": "npx ts-node -r tsconfig-paths/register --transpile-only src/lib/crypto.ts"
  },
  "dependencies": {
    "@google-cloud/tasks": "3.1.1",
    "@solana/spl-token": "0.3.7",
    "@solana/web3.js": "1.73.3",
    "body-parser": "1.20.2",
    "crypto": "1.0.1",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "jsonwebtoken": "9.0.0",
    "node-fetch": "2.6.7"
  },
  "devDependencies": {
    "@types/express": "4.17.17",
    "@types/jest": "29.4.0",
    "@types/jsonwebtoken": "9.0.1",
    "@types/node": "18.14.2",
    "@types/node-fetch": "2.6.2",
    "@types/superagent": "4.1.16",
    "@types/supertest": "2.0.12",
    "babel-loader": "9.1.2",
    "esbuild": "0.17.10",
    "eslint": "8.35.0",
    "jest": "29.4.3",
    "nodemon": "2.0.20",
    "npm-check-updates": "16.7.10",
    "prettier": "2.8.4",
    "superagent": "8.0.9",
    "supertest": "6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "9.4.2",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.9.5"
  }
}
